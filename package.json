{
  "name": "vscode-pont",
  "displayName": "pont",
  "description": "",
  "version": "0.1.4",
  "publisher": "jasonHzq",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "pont-config.json",
        "url": "./configSchema.json"
      }
    ],
    "commands": [
      {
        "command": "pont.findInterface",
        "title": "find pont interface"
      }
    ],
    "keybindings": [
      {
        "command": "pont.findInterface",
        "key": "cmd+ctrl+p",
        "mac": "cmd+ctrl+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^2.9.2",
    "vsce": "^1.36.2",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "pont-engine": "0.1.3"
  },
  "repository": {
    "url": "git@github.com:nefe/vscode-pont.git",
    "type": "git"
  }
}
